  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npm run typeorm -- migration:run && npm run db:seed",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=8192 dist/main",
    "prestart:prod": "npm run typeorm -- migration:run && npm run db:seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
