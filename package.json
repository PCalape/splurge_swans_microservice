  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@modules/(.*)": "<rootDir>/modules/$1",
      "@entities/(.*)": "<rootDir>/data/entities/$1",
      "@schemas/(.*)": "<rootDir>/data/schemas/$1",
      "@enums/(.*)": "<rootDir>/data/enums/$1",
      "@adminEnums/(.*)": "<rootDir>/data/enums/admin/enums/$1"
    }
  }
